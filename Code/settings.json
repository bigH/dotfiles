{
    "workbench.colorTheme": "GitHub Dark Default",
    "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "editor.tabSize": 2,
    "editor.detectIndentation": false,
    "[ruby]": {
        "editor.defaultFormatter": "Shopify.ruby-lsp",
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.insertSpaces": true,
        "files.trimTrailingWhitespace": true,
        "files.insertFinalNewline": true,
        "files.trimFinalNewlines": true,
        "editor.rulers": [
            120
        ],
        "editor.semanticHighlighting.enabled": true,
        "editor.indentSize": 2,
        "editor.tabSize": 4,
    },
    "[python]": {
        "editor.indentSize": 4,
        "editor.tabSize": 4,
        "editor.semanticHighlighting.enabled": true,
        "editor.insertSpaces": true,
        "files.trimTrailingWhitespace": true,
        "files.insertFinalNewline": true,
        "files.trimFinalNewlines": true,
        "editor.rulers": [
            88
        ],
    },
    "editor.fontFamily": "Hasklug",
    "editor.fontSize": 13,
    "github.copilot.chat.welcomeMessage": "always",
    "github.copilot.chat.scopeSelection": true,
    "terminal.integrated.macOptionIsMeta": true,
    "editor.lineNumbers": "relative",
    "terminal.integrated.copyOnSelection": true,
    "editor.guides.bracketPairs": true,
    "editor.autoClosingBrackets": "beforeWhitespace",
    "editor.stickyScroll.maxLineCount": 10,
    "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
    "javascript.inlayHints.parameterNames.enabled": "all",
    "javascript.inlayHints.parameterTypes.enabled": true,
    "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
    "javascript.inlayHints.variableTypes.enabled": true,
    "typescript.inlayHints.enumMemberValues.enabled": true,
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
    "typescript.inlayHints.parameterNames.enabled": "all",
    "typescript.inlayHints.parameterTypes.enabled": true,
    "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
    "typescript.inlayHints.variableTypes.enabled": true,
    "Lua.hint.enable": true,
    "editor.cursorBlinking": "solid",
    "indentRainbow.ignoreErrorLanguages": [
        "markdown"
    ],
    "protoc": {
        "options": [
            "--proto_path=<path of your proto files>"
        ]
    },
    "editor.wrappingIndent": "indent",
    "editor.minimap.renderCharacters": false,
    "peacock.favoriteColors": [
        {
            "name": "Red",
            "value": "#70261F"
        },
        {
            "name": "Orange",
            "value": "#7A5227"
        },
        {
            "name": "Yellow",
            "value": "#706D1F"
        },
        {
            "name": "Green",
            "value": "#397822"
        },
        {
            "name": "Blue",
            "value": "#2E6187"
        },
         {
            "name": "Indigo",
            "value": "#264569"
        },
        {
            "name": "Violet",
            "value": "#4A2469"
        },
        {
            "name": "Grey",
            "value": "#111111"
        }
    ],
    "gitlens.views.branches.branches.layout": "list",
    "window.zoomLevel": 1,
    "editor.renderWhitespace": "all",
    "indentRainbow.colors": [
        "rgba(255,255,64,0.07)",
        "rgba(127,255,127,0.07)",
        "rgba(255,127,255,0.07)",
        "rgba(79,236,236,0.07)"
    ],
    "editor.accessibilitySupport": "off",
    "rubyLsp.linters": null,
    "ruby.rubocop.onSave": false,
    "editor.defaultFormatter": null,
    "go.toolsManagement.autoUpdate": true,
    "copyOnSelect.trimStart": true,
    "terminal.external.osxExec": "iTerm2.app",
    "terminal.integrated.fontSize": 14,
    "git.openRepositoryInParentFolders": "always",
    "makefile.configureOnOpen": true,
    "circleci.apiToken": "${env:CIRCLECI_API_TOKEN}",
    "circleci.ssh.privateKey.github": "~/.ssh/id_ed25519",
    "circleci.hostUrl": "",
    "circleci.notifications.mute": true,
    "git.decorations.enabled": false,
    "git.countBadge": "off",
    "git.detectSubmodules": false,
    "git.enabled": false,
    "git.enableStatusBarSync": false,
    "git.optimisticUpdate": false,
    "git.requireGitUserConfig": false,
    "git.showInlineOpenFileAction": false,
    "git.showProgress": false,
    "github.branchProtection": false,
    "github.gitProtocol": "ssh"
}
