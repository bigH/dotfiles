#!/usr/bin/env bash

usage() {
  echo 'TODO'
}

error_exit_usage() {
  echo 'TODO'
}

get() {
  echo 'TODO' "$@"
}

ACTION="$1"

case $ACTION in
  get)
    get "$@"
    ;;
  select)
    get "$@" | as_lines "$@" | fzf_wrapper "$@"
    ;;
  *)
    error_exit_usage "Unknown action: $ACTION"
    ;;
esac
